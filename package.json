{
    "name": "derw-syntax",
    "displayName": "derw-syntax",
    "description": "Syntax highlighting for Derw",
	"author": "eeue56",
    "repository": "https://github.com/derw-lang/derw-syntax",
    "publisher": "derw",
    "keywords": [ "derw", "syntax", "highlighting" ],
    "sponsor": {
        "url": "https://github.com/sponsors/eeue56"
    },
    "pricing": "Free",
    "icon": "derw_logo.png",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.58.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "derw",
                "aliases": [
                    "Derw",
                    "derw"
                ],
                "extensions": [
                    ".derw"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "derw",
                "scopeName": "source.derw",
                "path": "./syntaxes/derw-syntax.json"
            }
        ]
    }
}
